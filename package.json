{
  "name": "arazzo-generator",
  "version": "0.0.4",
  "description": "Generates Arazzo Workflow Documents from your OpenAPI Documents",
  "main": "index.js",
  "scripts": {
    "test": "mocha --config './test/.mocharc.js'",
    "test-specific": "mocha --config './test/.mocharc.js' --grep 'generates workflows with global security and optional securtity at operation level'",
    "test-coverage": "nyc mocha --config './test/.mocharc.js'",
    "prepare": "husky",
    "start": "node cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaredCE/Arazzo-Generator.git"
  },
  "keywords": [
    "Arazzo",
    "OpenAPI",
    "Workflows"
  ],
  "bin": {
    "arazzo-generator": "cli.js"
  },
  "author": "Jared Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JaredCE/Arazzo-Generator/issues"
  },
  "homepage": "https://github.com/JaredCE/Arazzo-Generator#readme",
  "devDependencies": {
    "chai": "^6.2.2",
    "husky": "^9.1.7",
    "mocha": "^11.7.5",
    "nock": "^14.0.10",
    "nyc": "^17.1.0",
    "sinon": "^21.0.1"
  },
  "dependencies": {
    "@redocly/openapi-core": "^2.15.0",
    "stream-chain": "^3.4.0",
    "stream-json": "^1.9.1"
  }
}
